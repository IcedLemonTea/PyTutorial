import random

num = random.randint(1, 10)
guess = ''
guess_count = 0
guess_limit = 5
out_of_guesses = False

while guess != num and not out_of_guesses:
    if guess_count < guess_limit:
        guess = int(input('Enter your guess here: '))
        guess_count += 1
        if guess == num:
            print('Congratulations you win')
        elif guess > num:
            print('Too High')
        elif guess < num:
            print('Too Low')
    else:
        out_of_guesses = True

if out_of_guesses:
    print('Sorry, try again')
